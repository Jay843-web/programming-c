#include<stdio.h>
int main()
{
  int var1,i;
  printf("this is designed by nikhil bhardwaj ");
  printf("hello user..... ! please enter any number:");
  scanf("%d",&var1);
  printf("\n table of the entered number is");
  for(i=0; i<=10; i++) 
  {
    printf("\n%d*%d=%d",var1,i,var1*i);
  }
  return 0;
}
